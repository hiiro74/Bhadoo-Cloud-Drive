{
    "name": "SaveToGoogleDrive",
    "description": "App to Upload files directly to Google Drive",
    "repository": "https://raw.githubusercontent.com/hiiro74/url-to-cloud",
    "logo": "https://raw.githubusercontent.com/hiiro74/url-to-cloud/master/src/main/resources/static/img/savetodrive.png",
    "keywords": ["google","drive","cloud","cloud transfer","save to drive"],
    "env": {
        "client_id": {
            "description": "Client ID from Google Console (https://tinyurl.com/suew4nr)",
            "value": "XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX.apps.googleusercontent.com"
        },
        "client_secret": {
            "description": "Client Secret from Google Console (https://tinyurl.com/suew4nr)",
            "value": "XXXXXXXXXXXXXXXXXXXXXXXX"
        },
        "redirect_uri": {
            "description": "Your App URL eg. https://appname.heroku.com/api/oauth/google/callback (Replace appname with your own)",
            "value": "https://savetogoogledrive.herokuapp.com/api/oauth/google/callback"
        }
    }
}
